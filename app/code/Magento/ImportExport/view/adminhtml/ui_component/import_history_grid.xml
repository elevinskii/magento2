<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">import_history_grid.import_history_grid_data_source</item>
        </item>
    </argument>
    <settings>
        <spinner>import_history_grid_columns</spinner>
        <deps>
            <dep>import_history_grid.import_history_grid_data_source</dep>
        </deps>
    </settings>
    <dataSource name="import_history_grid_data_source" component="Magento_Ui/js/grid/provider">
        <settings>
            <storageConfig>
                <param name="indexField" xsi:type="string">history_id</param>
            </storageConfig>
            <updateUrl path="mui/index/render"/>
        </settings>
        <dataProvider class="Magento\ImportExport\Ui\DataProvider\ImportHistoryDataProvider"
                      name="import_history_grid_data_source">
            <settings>
                <requestFieldName>id</requestFieldName>
                <primaryFieldName>history_id</primaryFieldName>
            </settings>
        </dataProvider>
    </dataSource>
    <columns name="import_history_grid_columns">
        <column name="history_id">
            <settings>
                <label translate="true">ID</label>
                <sorting>desc</sorting>
            </settings>
        </column>
        <column name="started_at"
                class="Magento\Ui\Component\Listing\Columns\Date"
                component="Magento_Ui/js/grid/columns/date">
            <settings>
                <label translate="true">Start Date&amp;Time</label>
            </settings>
        </column>
        <column name="user_id">
            <settings>
                <label translate="true">User</label>
            </settings>
        </column>
        <column name="imported_file">
            <settings>
                <label translate="true">Imported File</label>
                <sortable>false</sortable>
            </settings>
        </column>
        <column name="error_file">
            <settings>
                <label translate="true">Error File</label>
                <sortable>false</sortable>
            </settings>
        </column>
        <column name="execution_time">
            <settings>
                <label translate="true">Execution Time</label>
            </settings>
        </column>
        <column name="summary">
            <settings>
                <label translate="true">Summary</label>
            </settings>
        </column>
    </columns>
</listing>
